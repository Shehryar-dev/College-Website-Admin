    <!-- START CONTENT -->
    <section id="main-content">
        <section class="wrapper main-wrapper">

            <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                <div class="page-title">

                    <div class="pull-left">
                        <h1 class="title" style="font-family: Pacifico;color: #fffffe;"><span style="color: #26b75e;">PREVIOUS</span> EDUCATION</h1>
                    </div>

                    <div class="pull-right hidden-xs">
                        <ol class="breadcrumb" style="font-family: Pacifico;color: 	#fffffe;">
                            <li>
                                <a asp-action="Index" asp-controller="Admin" style="font-family: 'Almendra';color: #fffffe;">
                                    <i class="fa fa-home"></i>Home
                                </a>
                            </li>
                            <li>
                                <a asp-action="education_record" asp-controller="Admin" style="font-family: 'Almendra';color: #fffffe;">Students</a>
                            </li>
                            <li class="active" style="font-family: 'Almendra';color: #fffffe;">
                                <strong>Previous Education</strong>
                            </li>
                        </ol>
                    </div>

                </div>
            </div>
            <div class="clearfix"></div>


            <div class="col-lg-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Student Previous Education Data</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle fa fa-chevron-down"></i>
                            <i class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></i>
                            <i class="box_close fa fa-times"></i>
                        </div>
                    </header>
                    <div class="content-body">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <table class="table table-bordered text-center">
                                    <thead>
                                        <tr>
                                            <th class=" text-center">ID</th>
                                            <th class=" text-center">Std Name</th>
                                            <th class=" text-center">Std Email</th>
                                            <th class=" text-center">Institute</th>
                                            <th class=" text-center">Exam</th>
                                            <th class=" text-center">Obtained Marks</th>
                                            <th class=" text-center">Total Marks</th>
                                            <th class=" text-center">Class Obtained</th>
                                            <th class=" text-center">Passing Year</th>
                                            <th class=" text-center">Login id</th>
                                            <th class=" text-center">Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>

                                                <td>
                                                    @item.EducationId
                                                </td>
                                                <td>
                                                    @item.Student.StudentsName
                                                </td>
                                                <td>
                                                    @item.Student.StudentsEmail
                                                </td>
                                                <td>
                                                    @item.InstituteName
                                                </td>
                                                <td>
                                                    @item.ExamName
                                                </td>
                                                <td>
                                                    @item.MarksObtained
                                                </td>
                                                <td>
                                                    @item.TotalMarks
                                                </td>
                                                <td>
                                                    @item.ClassObtained
                                                </td>
                                                <td>
                                                    @item.PassingYear
                                                </td>
                                                <td>
                                                    @item.AdmissionUser.UserId
                                                </td>
                                                <td>
                                                    <a asp-route-id="@item.EducationId" asp-action="education_del" asp-controller="Admin" class="btn btn-danger">
                                                        <i class="fa-regular fa-trash-can"></i>
                                                    </a>
                                                </td>

                                            </tr>
                                        }


                                    </tbody>
                                </table>
                            </div>
                        </div>




                    </div>
                </section>
            </div>
        </section>
    </section>
    <!-- END CONTENT -->
    <!-- END CONTENT -->
